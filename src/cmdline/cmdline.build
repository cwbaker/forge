
buildfile "cmdline_test/cmdline_test.build";

for _, build in build:default_builds("cc_.*") do
    build:Library '${lib}/cmdline_${architecture}' {
        build:Cxx '${obj}/%1' {
            "AddOption.cpp",
            "Error.cpp",
            "Option.cpp",
            "Parser.cpp",
            "Splitter.cpp"
        };
    };
end
