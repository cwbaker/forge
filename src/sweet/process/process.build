
buildfile "process_test/process_test.build";

local settings = build:current_settings();
for _, architecture in ipairs(settings.architectures) do
    build:Library ("process", architecture) {
        libraries = {
            "sweet/assert/assert",
            "sweet/error/error"
        };

        build:Cxx (architecture) {
            precompiled_header = "stdafx.hpp";
            "Error.cpp",
            "Environment.cpp",
            "Process.cpp"
        };
    };
end
