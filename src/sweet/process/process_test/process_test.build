
local settings = build.current_settings();
for _, architecture in ipairs(settings.architectures) do
    Executable ("process_test", architecture) {
        libraries = {
            "sweet/process/process",
            "sweet/cmdline/cmdline",
            "sweet/error/error",
            "sweet/assert/assert"
        };

        Cxx (architecture) {
            precompiled_header = "stdafx.hpp";
            "Application.cpp",
            "main.cpp"
        };
    };
end
